<template>
  <widget>
    <!-- Normal bind -->
    <label v-bind:text="label_text"/>
    <!-- Shortcut for bind -->
    <label :text="label_text"/>
  </widget>
</template>

<script lang="python">
import collagraph as cg


class Labels(cg.Component):
    def __init__(self, props):
        super().__init__(props)
        self.state["label_text"] = props.get("text", "Label")
</script>
